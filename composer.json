{
  "name": "drupal/composer_security_checker",
  "type": "drupal-module",
  "description": "Check for security updates to Composer packages using sensiolabs/security-checker",
  "authors": [
    {
      "name": "Matt Chapman",
      "email": "chapabu101@gmail.com",
      "homepage": "https://github.com/Chapabu"
    }
  ],
  "autoload": {
    "psr-4": {
      "Drupal\\composer_security_checker\\": "src"
    }
  },
  "keywords": ["Drupal"],
  "license": "GPL-2.0+",
  "homepage": "https://www.drupal.org/project/composer_security_checker",
  "minimum-stability": "dev",
  "support": {
    "issues": "http://drupal.org/project/issues/composer_security_checker",
    "source": "http://cgit.drupalcode.org/composer_security_checker"
  },
  "require": {
    "sensiolabs/security-checker": "3.0.*",
    "roave/security-advisories": "dev-master"
  },
  "require-dev": {
    "phpspec/phpspec": "~2.0"
  }
}
